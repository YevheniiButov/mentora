# reCAPTCHA Configuration
# Получите ключи на https://www.google.com/recaptcha/admin

# Включить reCAPTCHA (true/false)
RECAPTCHA_ENABLED=true

# Публичный ключ (Site Key) - используется в HTML
RECAPTCHA_PUBLIC_KEY=6LcXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Приватный ключ (Secret Key) - используется на сервере
RECAPTCHA_PRIVATE_KEY=6LcXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Домены для reCAPTCHA (через запятую)
RECAPTCHA_DOMAINS=mentora.com.in,www.mentora.com.in,mentora-nl.onrender.com,bigmentor.nl,www.bigmentor.nl
